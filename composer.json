{
    "name": "benmatselby/ringli",
    "description": "Little CLI tool to get some CircleCI data",
    "require": {
        "symfony/console": "^7.2",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^11.5.3"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "Ringli\\": "src/"
        }
    }
}
